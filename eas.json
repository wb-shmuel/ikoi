{
  "cli": {
    "version": ">= 16.18.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "ios": {
        "distribution": "store",
        "simulator": false
      },
      "android": {
        "buildType": "app-bundle"
      },
      "autoIncrement": true,
      "channel": "production"
    },
    "internal": {
      "ios": {
        "distribution": "internal",
        "simulator": false
      }
    }
  },
  "submit": {
    "production": {
    }
  }
}
