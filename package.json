{
  "name": "quick-calm",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android --clear",
    "ios": "expo start --ios --clear",
    "web": "expo start --web",
    "lint": "expo lint",
    "expo:login": "npx eas login",
    "expo:config:update": "npx eas update:configure",
    "expo:publish:prv": "npx eas update --channel preview",
    "ios:submit": "eas submit -p ios --path ./BeBored.ipa --profile production",
    "android:submit": "eas submit -p android --path ./BeBored.aab --profile production",
    "ios:build": "npx eas build --profile production --platform ios --clear-cache",
    "android:build": "npx eas build --profile production --platform android --clear-cache"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "expo": "^54.0.10",
    "expo-audio": "^1.0.13",
    "expo-dev-client": "~6.0.12",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-keep-awake": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.8",
    "expo-screen-orientation": "~9.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "~29.0.11",
    "expo-video": "^3.0.11",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "^0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.1.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "replace-color": "^2.3.0",
    "sharp": "^0.34.3",
    "typescript": "~5.8.3"
  },
  "private": true
}
