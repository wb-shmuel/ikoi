{
  "name": "quick-calm",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android --clear",
    "ios": "expo start --ios --clear",
    "web": "expo start --web",
    "lint": "expo lint",
    "expo:login": "npx eas login",
    "expo:config:update": "npx eas update:configure",
    "expo:publish:prv": "npx eas update --channel preview",
    "ios:submit": "eas submit -p ios --path ./BeBored.ipa --profile production",
    "android:submit": "eas submit -p android --path ./BeBored.aab --profile production",
    "ios:build": "npx eas build --profile production --platform ios --clear-cache",
    "android:build": "npx eas build --profile production --platform android --clear-cache"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "expo": "~53.0.22",
    "expo-av": "~15.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-keep-awake": "~14.1.4",
    "expo-router": "~5.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.11",
    "expo-updates": "~0.28.17",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.6",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "expo-screen-orientation": "~8.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "replace-color": "^2.3.0",
    "sharp": "^0.34.3",
    "typescript": "~5.8.3"
  },
  "private": true
}
